{
  "name": "lottopass",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@lottopass/web",
    "dev:mobile": "turbo dev --filter=@lottopass/mobile",
    "build": "turbo build",
    "build:web": "turbo build --filter=@lottopass/web",
    "build:mobile": "turbo build --filter=@lottopass/mobile",
    "test": "turbo test",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/cheerio": "^0.22.35",
    "@vercel/node": "^5.3.7",
    "cheerio": "^1.1.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "turbo": "^2.0.12",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@dotlottie/player-component": "^2.7.12",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "antd": "^5.26.6",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.11",
    "html5-qrcode": "^2.3.8",
    "postcss-pxtorem": "^6.1.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "sass": "^1.89.2",
    "vite": "^6.3.5"
  }
}
